{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
        "type": "webPubSubConnection",
        "name": "connection",
        "hub": "fastChat",
        "userId": "{headers.x-ms-client-principal-name}",
        "direction": "in"
    }
  ],
  "scriptFile": "../dist/getPubSubConnection/index.js"
}
